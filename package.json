{
  "name": "nuxt-delay-hydration",
  "version": "1.2.0",
  "packageManager": "pnpm@8.5.0",
  "description": "Improve your Nuxt.js Google Lighthouse score by delaying hydration ⚡️",
  "author": "Harlan Wilton <harlan@harlanzw.com>",
  "license": "MIT",
  "homepage": "https://github.com/harlan-zw/nuxt-delay-hydration",
  "repository": {
    "type": "git",
    "url": "https://github.com/harlan-zw/nuxt-delay-hydration"
  },
  "bugs": "https://github.com/harlan-zw/nuxt-delay-hydration/issues",
  "keywords": [
    "nuxt",
    "nuxt-module",
    "hydration",
    "lighthouse",
    "page speed",
    "performance"
  ],
  "exports": {
    "require": "./dist/module.cjs",
    "import": "./dist/module.mjs"
  },
  "main": "./dist/module.cjs",
  "types": "./dist/module.d.ts",
  "files": [
    "dist"
  ],
  "build": {
    "entries": [
      {
        "builder": "mkdist",
        "input": "./src/template/"
      }
    ]
  },
  "scripts": {
    "dev": "nuxi dev .playground",
    "stub": "nuxt-module-build --stub",
    "build": "nuxi prepare .playground && nuxt-module-build",
    "lint": "eslint . --fix",
    "release": "bumpp package.json packages/*/package.json --commit --push --tag",
    "test": "true"
  },
  "dependencies": {
    "@nuxt/kit": "^3.5.1",
    "@types/lodash.template": "^4.5.1",
    "lodash.template": "^4.5.0",
    "packrup": "^0.1.0",
    "radix3": "^1.0.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.39.3",
    "@nuxt/module-builder": "^0.3.1",
    "@nuxt/schema": "^3.5.1",
    "@nuxt/test-utils": "^3.5.1",
    "@nuxt/types": "^2.16.3",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^20.2.3",
    "@vitest/ui": "^0.31.1",
    "bumpp": "^9.1.0",
    "eslint": "^8.41.0",
    "nuxt": "^3.5.1",
    "typescript": "^5.0.4",
    "upath": "^2.0.1",
    "vitest": "^0.31.1"
  }
}
